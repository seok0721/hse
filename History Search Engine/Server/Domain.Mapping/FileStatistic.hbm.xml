<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Server" namespace="Server.Domain">
  <class name="FileStatistic" table="TBL_FILE_IO_STAT" >
    <composite-id>
      <key-property name="FileId"   column="FILE_ID" type="int" />
      <key-property name="Sequence" column="SEQ"     type="int" />
    </composite-id>
    <property name="FirstIODateTime" column="FIRST_IO_DT" type="DateTime" />
    <property name="LastIODateTime"  column="LAST_IO_DT"  type="DateTime" />
    <property name="IOCount"         column="IO_CNT"      type="int"      />
  </class>
</hibernate-mapping>