<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Server" namespace="Server.Domain">
  <class name="FileIOStat" table="TBL_FILE_IO_STAT" >
    <composite-id>
      <key-property name="ID"       column="FILE_ID"     type="int" />
      <key-property name="Sequence" column="IO_STAT_SEQ" type="int" />
    </composite-id>
    <property name="IOCount"    column="IO_CNT"       type="int"      />
    <property name="HeadIOTime" column="HEAD_IO_TIME" type="DateTime" />
    <property name="TailIOTime" column="TAIL_IO_TIME" type="DateTime" />
  </class>
</hibernate-mapping>